{
  "version": "1.0.2",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "strapi-v5",
    "strapi-custom-field",
    "icon-picker",
    "icon-input",
    "svg-picker",
    "custom-input",
    "strapi-design-system",
    "strapi-admin",
    "icon-library",
    "iconify",
    "iconify-icons",
    "iconify-integration",
    "svg-icons",
    "visual-icon-picker",
    "icon-selector",
    "frontend-component",
    "strapi-icon-picker"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@iconify/react": "^5.2.0",
    "@strapi/design-system": "^2.0.0-rc.18",
    "@strapi/icons": "^2.0.0-rc.18",
    "@tailwindcss/vite": "^4.0.15",
    "@types/lodash.debounce": "^4.0.9",
    "lodash.debounce": "^4.0.8",
    "react-intl": "^7.1.7",
    "tailwindcss": "^4.0.15"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.11.3",
    "@strapi/typescript-utils": "^5.11.3",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.16",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.16"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-plugin-iconhub",
    "displayName": "IconHub",
    "description": "Icon picker plugin for Strapi with Iconify integration — search, select, and store icons with raw SVG for high-performance rendering."
  },
  "name": "@arshiash80/strapi-plugin-iconhub",
  "description": "Icon picker plugin for Strapi with Iconify integration — search, select, and store icons with raw SVG for high-performance rendering.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Arshiash80/strapi-plugin-iconhub.git"
  },
  "bugs": {
    "url": "https://github.com/Arshiash80/strapi-plugin-iconhub/issues"
  },
  "homepage": "https://github.com/Arshiash80/strapi-plugin-iconhub#readme",
  "author": "Arshia Shahidi <admin@arshiash80.com>"
}
